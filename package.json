{
  "name": "ai-2dor-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm -r --workspace-root=false --parallel build",
    "build:web": "pnpm --filter web build",
    "lint": "cd packages/core && npx prisma generate && cd ../.. && pnpm -r --workspace-root=false lint",
    "test": "pnpm -r --workspace-root=false test",
    "format": "pnpm -r --workspace-root=false format",
    "smoke": "start-server-and-test \"PORT=3001 pnpm dev:web\" http://localhost:3001/api/health \"echo 'Smoke test passed!'\""
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "start-server-and-test": "^2.1.2",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  }
}
